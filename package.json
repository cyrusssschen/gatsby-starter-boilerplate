    "enzyme": "^3.11.0",
    "enzyme-adapter-react-16": "^1.15.2",
    "gatsby-plugin-sass": "^2.1.30",
    "gh-pages": "^2.2.0",
    "http-server": "^0.12.1",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^25.1.0",
    "node-sass": "^4.13.1",
    "prettier": "^1.19.1",
    "showdown": "^1.9.1"
